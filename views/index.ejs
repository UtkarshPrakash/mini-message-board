<html>
    <head>
        <title><%= title %></title>
        <link rel="stylesheet" href="./styles.css">
    </head>
    <body>
        <% for (let i=0; i<messages.length; i++) { %>
            <div class="card">
                <p class="element"> <%= messages[i].text %> </p>
                <p class="element"> <%= messages[i].user %> </p>
                <p class="element"> <%= messages[i].added %> </p>
            </div>
            <br>
        <% } %>
        <a href="/new">
            <button class="button">Add new user</button>
        </a>
    </body>
</html>